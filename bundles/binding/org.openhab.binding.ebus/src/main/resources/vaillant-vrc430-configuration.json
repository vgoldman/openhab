        "values": {
            "temp_outside": {"type": "data2c", "pos": 12, "label": "Outside temperature"},
            "status":       {"type": "uchar", "pos": 14, "label": "Outside temperature status",
                "mapping":  {"0":"Ok", "85":"Circuit", "170":"Cutoff"}}
        }
    },

